{
    "manifest_version": 2,

    "name": "ReplyReminder",
    "description": "Remind yourself to follow up",
    "version": "0.0.1",
    "content_scripts": [{
        "matches": ["https://www.facebook.com/*", "https://www.messenger.com/*"],
        "js": ["content_script.js"]
    }],
    "web_accessible_resources": [
        "icon.png"
    ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": []
}
